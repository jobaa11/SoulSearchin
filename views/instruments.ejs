<%- include('./partials/header') %>


    <form id="instru" class="allapp" type="submit" action="/instruments" method="POST">
        
        <ul>&nbsp;&nbsp;
        <label id="instrument-label" for="instruments"><u>Instruments</u></label>
            <% instrument.forEach(function(i) { %>
                <li class="instrument-list" value="<%= i._id %>">
                    <%= i.name %>
                </li>
                <% }) %>
                <input type="text" name="name" id="add" autocomplete="off" autocapitalize="words">
                <button id="instrument-btn" type="submit" >ADD</button>
            </ul>
    </form>

    <%- include('./partials/footer') %>