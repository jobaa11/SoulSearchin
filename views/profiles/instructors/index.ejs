<%- include('../../partials/header') %>



    <% if (user) { %>
        <% if (profile && profile.isInstructor) { %>
            <section id="show-page">
                <div>
                    <div class="name-input">Name:
                        <%= profile.nickname %>
                    </div>
                </div>
                <div>
                    <div class="location-input">Location:
                        <%= profile.location %>
                    </div>
                </div>
                <div>
                    <div>Bio:
                        <%= profile.bio %>
                    </div>
                </div>
                <div>Instruments:
                    <div>
                        <% profile.instruments.forEach(function(instrument) { %>

                            <%= instrument.name %>

                                <% }) %>
                    </div>
                </div>
                <div>Genres:
                    <div>
                        <%= profile.genres %>
                    </div>
                </div>
                <div>Budget:
                <div>
                    $<%= profile.budget %>/HR
                </div>
                </div>
                <a class="update-profile-link" href="/profiles/<%= profile._id%>/edit">UPDATE PROFILE</a>
            </section>

            <% } %>

                <% } %>



                    <%- include('../../partials/footer') %>